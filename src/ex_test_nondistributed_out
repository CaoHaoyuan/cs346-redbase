
REDBASE >> create table major (majorid = c5, name = c50, deptid = c5);

REDBASE >> create table class (classid = i, name = c21, deptid = c5);

REDBASE >> create table dept (deptid = c5, name = c50);

REDBASE >> create table enroll (studentid = i, classid = i, quarter = c7, year = i, grade = c3);

REDBASE >> create table student (studentid = i, name = c21, majorid = c6, gpa = f);

REDBASE >> create index major(majorid);

REDBASE >> create index student(studentid);

REDBASE >> create index class(classid);

REDBASE >> create index dept(deptid);

REDBASE >> load major("../data/EX/major.data");

REDBASE >> load student("../data/EX/student.data");

REDBASE >> load enroll("../data/EX/enroll.data");

REDBASE >> load class("../data/EX/class.data");

REDBASE >> load dept("../data/EX/dept.data");

REDBASE >> print dept;
